<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.snsh.dev.Willmel">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />


    <application
        android:name="com.snsh.dev.Willmel.App"
        android:allowBackup="true"
        android:icon="@drawable/favicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:windowSoftInputMode="stateHidden"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.snsh.dev.Willmel.activity.MainActivity"
            android:windowSoftInputMode="stateHidden"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".activity.BestDealHome_SeeMore"
            android:label="Best Deal Home See More"
            android:parentActivityName=".activity.MainActivity">
        </activity>

        <activity
            android:name=".activity.PeopleYouMay_SeeMore"
            android:label="People You May See More"
            android:windowSoftInputMode="stateHidden"
            android:parentActivityName=".activity.MainActivity">
        </activity>

    </application>

</manifest>